{
	"name": "@custom-bits/eslint-config-airbnb-flat",
	"type": "module",
	"version": "0.0.0",
	"description": "Unofficial port of eslint airbnb styleguide to eslint-flat-config",
	"author": "error-four-o-four",
	"license": "MIT",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-flat-config",
		"airbnb",
		"styleguide",
		"unofficial"
	],
	"exports": {
		"./base": {
			"default": "./src/base.js"
		},
		"./configs": {
			"default": "./src/config/index.js"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"convert": "node ./scripts/convert.js",
		"write:configs": "node ./scripts/writeConfigs.js",
		"write:deprecated": "node ./scripts/writeDeprecated.js",
		"write:ts": "node ./scripts/writeTypescript.js",
		"prettier:check": "prettier . --check --ignore-unknown",
		"lint:check": "eslint ."
	},
	"dependencies": {
		"eslint": "^8.55.0"
	},
	"peerDependencies": {
		"@stylistic/eslint-plugin": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"eslint-plugin-i": "^2.29.0",
		"eslint-plugin-n": "^16.3.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^2.1.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-flat-config-viewer": "^0.1.3",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.0"
	}
}