{
	"name": "eslint-config-airbnb-flat",
	"type": "module",
	"version": "0.0.0",
	"description": "Unofficial port of the airbnb styleguide from eslintrc format to flat config file format",
	"author": "error-four-o-four",
	"license": "MIT",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-flat-config",
		"airbnb",
		"styleguide",
		"unofficial"
	],
	"files": [
		"src"
	],
	"exports": {
		"./base": {
			"default": "./src/base.js"
		},
		"./stylistic": {
			"default": "./src/stylistic.js"
		},
		"./compat": {
			"import": "./src/configs/airbnb/index.js"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"prettier:check": "prettier . --check --ignore-unknown",
		"prettier": "prettier . --write --ignore-unknown",
		"lint:check": "eslint .",
		"lint": "eslint . --fix",
		"generate:names": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/generateNames.ts",
		"generate:shared": "tsup-node scripts/types.ts --format esm --dts --dts-only -d ./shared",
		"generate:configs": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/generateConfigs.ts",
		"clean": "rimraf dist/",
		"// build:test": "tsup-node scripts/test.ts --format esm,cjs --legacy-output --dts --minify"
	},
	"peerDependencies": {
		"@stylistic/eslint-plugin": "^1.5.1",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"eslint": "^8.56.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-i": "^2.29.0",
		"eslint-plugin-n": "^16.4.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^2.1.4",
		"@tsconfig/node18": "^18.2.2",
		"@types/eslint": "^8.44.9",
		"@types/eslint__eslintrc": "^2.1.1",
		"@types/node": "^20.10.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-flat-config-viewer": "^0.1.3",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.1",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"@stylistic/eslint-plugin-js": "^1.5.1",
		"globals": "^13.24.0",
		"local-pkg": "^0.5.0"
	}
}