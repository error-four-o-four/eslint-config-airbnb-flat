{
	"name": "@custom-bits/eslint-config-airbnb-flat",
	"type": "module",
	"version": "0.0.0",
	"description": "Unofficial port of airbnb styleguide eslintrc files to flat config file format",
	"author": "error-four-o-four",
	"license": "MIT",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-flat-config",
		"airbnb",
		"styleguide",
		"unofficial"
	],
	"files": [
		"src"
	],
	"exports": {
		"./base": {
			"default": "./src/base.js"
		},
		"./stylistic": {
			"default": "./src/stylistic.js"
		},
		"./compat": {
			"import": "./src/configs/airbnb/index.js"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"convert": "node ./scripts/index.js",
		"prettier:check": "prettier . --check --ignore-unknown",
		"prettier": "prettier . --write --ignore-unknown",
		"lint:check": "eslint .",
		"lint": "eslint . --fix"
	},
	"peerDependencies": {
		"@stylistic/eslint-plugin": "^1.5.0",
		"@stylistic/eslint-plugin-js": "^1.5.1",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"eslint": "^8.55.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-i": "^2.29.0",
		"eslint-plugin-n": "^16.3.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^2.1.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-flat-config-viewer": "^0.1.3",
		"eslint-plugin-prettier": "^5.0.1",
		"prettier": "^3.1.0"
	}
}